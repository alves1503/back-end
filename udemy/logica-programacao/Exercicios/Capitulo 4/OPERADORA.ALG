Algoritmo "operadora"



Var
          min : inteiro
          total : real

Inicio


          escreva("Digite a quantidade de minutos ")
          leia(min)
          
          total <- 50.0
          
          se min > 100 entao
            total <- total + 2 * (min-100)
          fimse
          
          
          escreval("Valor pago: ", total:4:2)
            





Fimalgoritmo